#!/usr/bin/env bash
#script that displays numbers from 1 to 100
for ((count=1;count<100;i++)); do
    if [ $((count % 15)) -eq 0 ]
    then
        printf "FizzBuzz\n"
    elif [ $((count % 3)) -eq 0 ]
    then
        printf "Fizz\n"
    elif [ $((count % 5)) -eq 0 ]
    then
        printf "Buzz\n"
    else
        echo "$count"
    fi
    ((count++))
done
